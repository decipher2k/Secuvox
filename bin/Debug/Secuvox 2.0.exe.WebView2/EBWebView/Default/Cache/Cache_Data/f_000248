(this.parseStyleToNumber(linkStyles.paddingLeft) || 0) + (this.parseStyleToNumber(linkStyles.marginLeft) || 0) + (this.parseStyleToNumber(linkStyles.paddingRight) || 0) + (this.parseStyleToNumber(linkStyles.marginRight) || 0));\n    const paddingBlock = ((this.parseStyleToNumber(linkStyles.paddingTop) || 0) + (this.parseStyleToNumber(linkStyles.marginTop) || 0) + (this.parseStyleToNumber(linkStyles.paddingBottom) || 0) + (this.parseStyleToNumber(linkStyles.marginBottom) || 0));\n    const hasPaddingBlock = paddingBlock > 0;\n\n    const extraBlockSpace = hasPaddingBlock ? 1 : 2;\n    const spaceToLeaveForLeftBubble = 8;\n    const spaceToLeaveForRightBubble = 85;\n\n    const elementConsumedSpace = {\n      height: this.parseStyleToNumber(linkStyles.height) * extraBlockSpace,\n      width: this.parseStyleToNumber(linkStyles.width) - paddingInline,\n      spacingLeft: this.parseStyleToNumber(linkStyles.paddingLeft) + this.parseStyleToNumber(linkStyles.marginLeft),\n      spacingRight: this.parseStyleToNumber(linkStyles.paddingRight) + this.parseStyleToNumber(linkStyles.marginRight),\n      spacingTop: this.parseStyleToNumber(linkStyles.paddingTop) + this.parseStyleToNumber(linkStyles.marginTop),\n      spacingBottom: this.parseStyleToNumber(linkStyles.paddingBottom) + this.parseStyleToNumber(linkStyles.marginBottom),\n    };\n\n    if (linkPosition.top < MIN_TOP_POSITION) {\n      this.panel.style.position = 'fixed';\n      document.body.style.overflow = 'hidden';\n      this.panel.style.top = `${linkPosition.top + elementConsumedSpace.height}px`;\n    } else {\n      this.panel.style.position = 'absolute';\n      document.body.style.overflow = 'auto';\n      this.panel.style.top = `${linkPosition.top + window.scrollY + elementConsumedSpace.height}px`;\n    }\n\n    if (this.isPanelGoingOutsideOfWindow(linkPosition)) {\n      this.panel.style.left = 'initial';\n      const spaceFromRight = window.innerWidth - linkPosition.right - elementConsumedSpace.width;\n      this.panel.style.right = `${spaceFromRight}px`;\n      const spaceFromRightForArrow = (this.parseStyleToNumber(panelStyles.width) * spaceToLeaveForRightBubble) / 100;\n      this.panel.style.setProperty('--left', `${spaceFromRightForArrow}px`);\n    } else {\n      const removalSpace = elementConsumedSpace.width <= 35 ? (elementConsumedSpace.width) : 0;\n      const spaceFromRight = (linkPosition.left + window.scrollX + elementConsumedSpace.spacingLeft) - removalSpace;\n      this.panel.style.left = `${spaceFromRight}px`;\n      const spaceFromLeftForArrow = (this.parseStyleToNumber(panelStyles.width) * spaceToLeaveForLeftBubble) / 100;\n      this.panel.style.setProperty('--left', `${spaceFromLeftForArrow}px`);\n    }\n    fadeIn(this.panel, 200);\n    this.panelOverlay = document.querySelector('.cleverpush-panel-overlay');\n    this.bindPanelOverlay();\n    this.showPanelContent();\n    this.triggerEvent(Event.PANEL_SHOWN);\n\n    if (this.api) {\n      this.api.trackPanel(null, 'opened');\n    }\n  }\n\n  bindPanelOverlay() {\n    if (this.panelOverlay) {\n      this.panelOverlay.addEventListener('click', () => this.hide());\n    }\n  }\n\n  hide() {\n    if (this.panel && this.panel.parentNode) {\n      this.panel.parentNode.removeChild(this.panel);\n    }\n\n    if (this.panelOverlay) {\n      this.panelOverlay.parentNode.removeChild(this.panelOverlay);\n      document.body.style.overflow = 'auto';\n    }\n\n    this.panel = null;\n    this.closeButton = null;\n    this.footerCloseButton = null;\n    this.tabButtons = null;\n    this.panelOverlay = null;\n\n    this.triggerEvent(Event.PANEL_HIDDEN);\n  }\n\n  onCloseClick(e) {\n    e.preventDefault();\n\n    this.hide();\n  }\n\n  onUnsubscribeClick(e) {\n    if (confirm(this.localization.confirm)) {\n      this.onCloseClick(e);\n      this.subscriptionManager.unsubscribe();\n    }\n  }\n\n  setTab(tab) {\n    this.tab = tab;\n    this.showPanelContent();\n  }\n\n  onTabButtonClick(e) {\n    logMethodCall('onTabButtonClick', e.currentTarget.dataset.tab);\n    e.preventDefault();\n    this.setTab(e.currentTarget.dataset.tab || 'notifications');\n  }\n\n  openNotification(e) {\n    e.preventDefault();\n    window.open(e.currentTarget.dataset.url, '_blank');\n    this.subscriptionManager.storageManager.setNotificationRead(e.currentTarget.dataset.id);\n    const isUnread = Boolean(e.currentTarget.dataset.read);\n    if (isUnread) {\n      this.updateNotificationCount();\n    }\n    this.showPanelContent();\n    if (this.api) {\n      this.api.trackPanel(e.currentTarget.dataset.id, 'clicked');\n    }\n  }\n\n  updateNotificationCount() {\n    let notificationCount = Number(localStorage.getItem(KEY_UNREAD_NOTIFICATIONS_COUNT));\n    if (notificationCount && this.bubbleSpeechLink) {\n      const countElelement = this.bubbleSpeechLink.firstChild;\n      notificationCount -= 1;\n      if (!notificationCount) {\n        this.bubbleSpeechLink.removeChild(countElelement);\n        this.bubbleSpeechLink.classList.remove(KEY_HAS_UNREAD_NOTIFICATIONS);\n        return;\n      }\n      countElelement.innerText = notificationCount;\n    }\n  }\n\n  getLocalization() {\n    let configBellLocalization = this.config.bellLocalization || {};\n    const language = detectLanguage(this.config || {});\n    if (this.config.bellLocalizationTranslations && language && this.config.bellLocalizationTranslations[language]) {\n      configBellLocalization = { ...configBellLocalization, ...this.config.bellLocalizationTranslations[language] };\n    }\n\n    const localization = {\n      notifications: charsetEscape(configBellLocalization.notifications) || translate('panel.notifications'),\n      settings: charsetEscape(configBellLocalization.settings) || translate('panel.settings'),\n      editTopics: charsetEscape(configBellLocalization.editTopics) || translate('panel.editTopics'),\n      notificationEmpty: charsetEscape(configBellLocalization.notificationEmpty) || translate('panel.notificationsEmpty'),\n      save: charsetEscape(configBellLocalization.save) || translate('panel.save'),\n      close: charsetEscape(configBellLocalization.close) || translate('panel.close'),\n      successfulSave: charsetEscape(configBellLocalization.successfulSave) || translate('panel.successfullySaved'),\n      unSubscribeNotification: charsetEscape(configBellLocalization.unSubscribeNotification) || translate('unsubscribe.title'),\n      confirm: charsetEscape(configBellLocalization.confirm) || translate('unsubscribe.confirm'),\n    };\n\n    return localization;\n  }\n\n  timeString(date) {\n    let result = date.toISOString().substring(0, 10);\n    result += ' ';\n    let hours = date.getHours();\n    if (hours < 10) {\n      hours = `0${hours}`;\n    }\n    let minutes = date.getMinutes();\n    if (minutes < 10) {\n      minutes = `0${minutes}`;\n    }\n    result += `${hours}:${minutes}`;\n    return result;\n  }\n\n  checkDateHour(date) {\n    return new Date() - date < HOURS;\n  }\n\n  checkIsToday(date) {\n    const today = new Date(Date.now()).setUTCHours(0, 0, 0, 0);\n    if (date === today) {\n      return true;\n    }\n    return false;\n  }\n\n  checkIsYesterday(date) {\n    const yesterday = new Date(Date.now() - DAY).setUTCHours(0, 0, 0, 0);\n    if (date === yesterday) {\n      return true;\n    }\n    return false;\n  }\n\n  getDaysDifference(date) {\n    return Math.abs(new Date() - Date.parse(date)) / DAY;\n  }\n\n  notificationDay = (notificationDate) => {\n    const timestamp = Date.parse(notificationDate);\n    const date = new Date(timestamp).setUTCHours(0, 0, 0, 0);\n    if (this.checkIsToday(date) || this.checkIsYesterday(date)) {\n      return this.getTodayAndYesterday(date);\n    }\n\n    if (this.getDaysDifference(notificationDate) > WEEK_DAY) {\n      return new Date(date).toLocaleString(this.language || DEFAULT_LANGUAGE, { dateStyle: 'long' });\n    }\n\n    return new Date(date).toLocaleString(this.language || DEFAULT_LANGUAGE, { weekday: 'long' });\n  };\n\n  groupToDay = (day, notifications) => ({\n    day,\n    notifications: notifications[day]\n  });\n\n  groupBy = (notifications) => {\n    const initialValue = {};\n    return notifications.reduce((acc, currentVal) => {\n      const myAttribute = this.notificationDay(currentVal.sentAt);\n      acc[myAttribute] = [...(acc[myAttribute] || []), currentVal];\n      return acc;\n    }, initialValue);\n  };\n\n  getTimeAgo(timestamp) {\n    const date = new Date(timestamp).setUTCHours(0, 0, 0, 0);\n    const rtf = new Intl.RelativeTimeFormat(this.language || DEFAULT_LANGUAGE, { numeric: 'auto' });\n    const dayOrHour = this.checkIsToday(date) || this.checkIsYesterday(date) ? HOURS : DAY;\n    const difference = Math.round((timestamp - Date.now()) / dayOrHour);\n    if (this.checkDateHour(date)) {\n      const minutesDiff = Math.round((timestamp - Date.now()) / MINUTES);\n      return rtf.format(minutesDiff, 'minutes');\n    }\n\n    if (this.checkIsToday(date) || this.checkIsYesterday(date)) {\n      return rtf.format(difference, 'hours');\n    }\n    return rtf.format(difference, 'day');\n  }\n\n  getTodayAndYesterday(date) {\n    const rtf = new Intl.RelativeTimeFormat(this.language || DEFAULT_LANGUAGE, { numeric: 'auto' });\n\n    const difference = Math.trunc((date - Date.now()) / DAY);\n    return rtf.format(difference, 'day');\n  }\n\n  showPanelContent() {\n    const panelContent = this.panel.querySelector('.cleverpush-panel-content');\n    const hidePanelNotifications = this.config.hidePanelNotifications;\n    const gearSettingOption = this.config.bellPanelGearButtonEnabled;\n    let content = '';\n    if (this.config.bellPanelTitle) {\n      content += `<div class=\"cleverpush-panel-title-section\"><div class=\"cleverpush-title\">${this.config.bellPanelTitle}</div><button class=\"cleverpush-panel-close\" type=\"button\">&times;</button></div>`;\n    }\n    content += !hidePanelNotifications && !gearSettingOption ? `<div class=\"cleverpush-panel-buttons\">\n      <button type=\"button\" class=\"cleverpush-panel-button${this.tab === 'notifications' ? ' active' : ''}\" data-tab=\"notifications\">${this.localization.notifications}</button>`\n      + `<button type=\"button\" class=\"cleverpush-panel-button${this.tab === 'settings' ? ' active' : ''}\" data-tab=\"settings\">${this.localization.settings}</button>`\n      + '</div>' : '';\n\n    if (this.tab === 'notifications') {\n      content += gearSettingOption ? `<div class=\"gear-setting-options\"><div>${this.localization.notifications}</div> <img data-tab=\"settings\" class\"cleverpush-panel-icon-button\" src=${getIconUrl('setting')} /> </div>` : '';\n    } else {\n      content += gearSettingOption ? `<div class=\"gear-setting-options\"><div>${this.localization.settings}</div> <img data-tab=\"notifications\" class\"cleverpush-panel-icon-button\" src=${getIconUrl('back')} /> </div>` : '';\n    }\n\n    if (this.config.notificationBellColor) {\n      content += `<style>.cleverpush-panel-button.active, .cleverpush-panel-settings button, .cleverpush-panel-settings button:hover, .cleverpush-panel-footer button, .cleverpush-panel-footer button:hover { background-color: ${this.config.notificationBellColor}; }</style>`;\n    }\n\n    if (!this.panel.classList.contains('cleverpush-inline-panel') && !this.config.bellPanelHeaderHidden) {\n      content += `\n      <div class=\"cleverpush-panel-header\" style=\"border-bottom-color: ${this.config.notificationBellColor || ''}\"><img src=\"${this.config.channelIcon}\"></div>\n      `;\n    }\n\n    const setPanelContent = (newContent) => {\n      panelContent.innerHTML = `${newContent}<div class=\"cleverpush-panel-footer\"><button type=\"button\" class=\"cleverpush-panel-footer-close\">${this.localization.close}</button><div class=\"cleverpush-panel-branding\">${this.getBrandingLinks()}</div></div>`;\n\n      this.footerCloseButton = this.panel.querySelector('.cleverpush-panel-footer-close');\n      if (this.footerCloseButton) {\n        this.footerCloseButton.addEventListener('click', this.onCloseClick.bind(this));\n      }\n\n      this.tabButtons = !gearSettingOption ? this.panel.querySelectorAll('.cleverpush-panel-button') : this.panel.querySelectorAll('.gear-setting-options img');\n      for (let i = 0; i < this.tabButtons.length; i += 1) {\n        const tabButton = this.tabButtons[i];\n        tabButton.addEventListener('click', this.onTabButtonClick.bind(this));\n      }\n\n      this.notificationItems = this.panel.querySelectorAll('.cleverpush-panel-notification');\n      for (let i = 0; i < this.notificationItems.length; i += 1) {\n        const notification = this.notificationItems[i];\n        notification.addEventListener('click', this.openNotification.bind(this));\n      }\n\n      this.closeButton = this.panel.querySelector('.cleverpush-panel-close');\n      if (this.closeButton) {\n        this.closeButton.addEventListener('click', this.onCloseClick.bind(this));\n      }\n    };\n\n    if (this.tab === 'notifications') {\n      content += '<div class=\"cleverpush-panel-spinner\"><div class=\"spinner\">\\n'\n        + '                <div class=\"double-bounce1\"></div>\\n'\n        + '                <div class=\"double-bounce2\"></div>\\n'\n        + '            </div></div>';\n      setPanelContent(content);\n      this.subscriptionManager.getStoredNotifications().then((notifications) => {\n        if (this.tab !== 'notifications') {\n          // user already switched tab fast enough\n          return;\n        }\n        this.subscriptionManager.storageManager.checkUnreadNotifications(notifications);\n        const groupNotifications = this.groupBy(notifications);\n        let unreadNotificationsCount = 0;\n        const result = Object.keys(groupNotifications).map((day) => this.groupToDay(day, groupNotifications)).sort((item) => item.notifications.sentAt);\n        this.subscriptionManager.storageManager.deleteOldNotificationsIds(notifications);\n        if (notifications.length) {\n          content += '<div class=\"cleverpush-panel-notifications\" style=\"display: none;\">';\n          result.forEach((item) => {\n            content += `<div class=\"cleverpush-panel-day\">${item.day}</div>`;\n            item.notifications.slice(0, 50).forEach((notification) => {\n              if (notification.testVariants && notification.testVariants.length && notification.testVariants[0]) {\n                if (!notification.title && notification.testVariants[0].title) {\n                  notification.title = notification.testVariants[0].title;\n                }\n                if (!notification.text && notification.testVariants[0].text) {\n                  notification.text = notification.testVariants[0].text;\n                }\n                if (notification.testVariants[0].url) {\n                  notification.url = notification.testVariants[0].url;\n                }\n              }\n\n              const date = new Date(Date.parse(notification.sentAt || notification.queuedAt || notification.scheduledAt || notification.createdAt));\n              const isRead = this.subscriptionManager.storageManager.isNotificationRead(notification._id);\n\n              if (!isRead && this.panel.classList.contains('cleverpush-inline-panel')) {\n                unreadNotificationsCount += 1;\n              }\n\n              content += `<div class=\"cleverpush-panel-notification ${isRead ? 'cleverpush-has-read' : 'cleverpush-has-unread'} ${this.checkDateHour(date) ? 'cleverpush-notification-current-hour' : ''}\" data-read=${isRead} data-url=\"${notification.url}\" data-id=\"${notification._id}\" style=\"border-left: ${isRead ? 'none' : `3px solid ${this.config.notificationBellColor || 'red'};`}\">`;\n\n              content += `<div class=\"cleverpush-panel-notification-image\" style=\"background-image: url('${notification.mediaUrl || notification.iconUrl || this.config.channelIcon}');\"></div>`;\n\n              content += `<div class=\"cleverpush-panel-notification-title\">${notification.title}</div>`;\n              if (notification.text) {\n                if (notification.text.length > 100) {\n                  notification.text = `${notification.text.substr(0, 100)}â€¦`;\n                }\n                content += `<div class=\"cleverpush-panel-notification-text\">${notification.text}</div>`;\n              }\n              if (notification.sentAt || notification.createdAt) {\n                const timestamp = Date.parse(notification.sentAt || notification.queuedAt || notification.scheduledAt || notification.createdAt);\n                content += `<div class=\"cleverpush-panel-notification-time\" style=\"color: ${isRead ? '' : this.config.notificationBellColor || ''}\">${this.getTimeAgo(timestamp) || ''}</div>`;\n              }\n              content += '</div>';\n              content += '<div class=\"line\"></div>';\n            });\n          });\n          content += '</div>';\n        } else {\n          content += `<div class=\"cleverpush-panel-notifications-empty\">${this.localization.notificationEmpty}</div>`;\n        }\n\n        if (!unreadNotificationsCount && this.bubbleSpeechLink) {\n          this.bubbleSpeechLink.classList.remove(KEY_HAS_UNREAD_NOTIFICATIONS);\n          sessionStorage.setItem(KEY_SPEECHBUBBLE_UNREAD_NOTIFICATIONS, false);\n        }\n\n        setPanelContent(content);\n\n        const spinner = panelContent.querySelector('.cleverpush-panel-spinner');\n        const notificationsDiv = panelContent.querySelector(notifications.length ? '.cleverpush-panel-notifications' : '.cleverpush-panel-notifications-empty');\n        if (spinner && notificationsDiv) {\n          fadeOut(spinner, 150, () => {\n            spinner.style.display = 'none';\n            notificationsDiv.style.opacity = 0;\n            notificationsDiv.style.display = 'block';\n            fadeIn(notificationsDiv, 150);\n          });\n        }\n      });\n    } else if (this.tab === 'settings') {\n     